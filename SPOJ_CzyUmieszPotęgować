#include<iostream>


using namespace std;


int main()
{

    int D,a,b;
    int tab[10][4]=
    {
        {0,0,0,0},
        {1,1,1,1},
        {6,2,4,8},
        {1,3,9,7},
        {6,4,6,4},     ///Warto zauważyć schemat liczb jedności w poszczególnych potęgach. Tablica przedstawia ten schemat.
        {5,5,5,5},
        {6,6,6,6},
        {1,7,9,3},
        {6,8,4,2},
        {1,9,1,9},
    };
    cin>>D;
    for(int i=0; i<D; i++)
    {
        cin>>a>>b;
        cout<<tab[a%10][b%4]<<endl;
    }
    return 0;
}
