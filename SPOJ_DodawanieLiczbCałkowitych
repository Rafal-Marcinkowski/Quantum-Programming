using System;

namespace SPOJ_DodawanieLiczbCa≈Çkowitych
{
    class Program
    {
        static void Main(string[] args)
        {
            int suma = 0;
            for (int i = 0; i < 3; i++)
            {
                int a = int.Parse(Console.ReadLine());
                suma += a;
            }
            Console.WriteLine(suma);
        }
    }
}
